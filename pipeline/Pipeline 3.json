{
	"name": "Pipeline 3",
	"properties": {
		"activities": [
			{
				"name": "Lookup1",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"dataset": {
						"referenceName": "Excel2",
						"type": "DatasetReference",
						"parameters": {
							"sheetIndex": {
								"value": "@pipeline().parameters.sheetIndex",
								"type": "Expression"
							},
							"filename": {
								"value": "@pipeline().parameters.filename",
								"type": "Expression"
							},
							"containername": {
								"value": "@pipeline().parameters.containername",
								"type": "Expression"
							},
							"exceldirectory": {
								"value": "@pipeline().parameters.exceldirectory",
								"type": "Expression"
							},
							"rangeofdatasetreportinfo": {
								"value": "@pipeline().parameters.rangeofdatasetreportinfo",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Set dataset name",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Lookup1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "datasetname",
					"value": {
						"value": "@string(activity('Lookup1').output.value[0].Prop_0)",
						"type": "Expression"
					}
				}
			},
			{
				"name": "If Condition1",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "Set reporting name",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@and(not(empty(variables('reportname'))), not(empty(variables('datasetname'))))",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "Data flow1",
							"type": "ExecuteDataFlow",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataflow": {
									"referenceName": "Dataflow4",
									"type": "DataFlowReference",
									"datasetParameters": {
										"source7": {
											"sheetIndex": {
												"value": "@pipeline().parameters.sheetIndex",
												"type": "Expression"
											},
											"filename": {
												"value": "@pipeline().parameters.filename",
												"type": "Expression"
											}
										},
										"sink1": {
											"csvfilename": {
												"value": "@concat(variables('datasetname'), '_', variables('reportname'), '.csv')",
												"type": "Expression"
											}
										}
									},
									"linkedServiceParameters": {
										"source7": {
											"schemaLinkedService": {
												"sheetIndex": {
													"value": "@pipeline().parameters.sheetIndex",
													"type": "Expression"
												},
												"filename": {
													"value": "@pipeline().parameters.filename",
													"type": "Expression"
												}
											}
										}
									}
								},
								"compute": {
									"coreCount": 8,
									"computeType": "General"
								},
								"traceLevel": "Fine"
							}
						}
					]
				}
			},
			{
				"name": "Set reporting name",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Set dataset name",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "reportname",
					"value": {
						"value": "@string(activity('Lookup1').output.value[1].Prop_0)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"sheetIndex": {
				"type": "int",
				"defaultValue": 1
			},
			"filename": {
				"type": "string",
				"defaultValue": "test.xlsx"
			},
			"containername": {
				"type": "string",
				"defaultValue": "hdofilesystem200"
			},
			"exceldirectory": {
				"type": "string",
				"defaultValue": "Excel"
			},
			"csvdirectory": {
				"type": "string",
				"defaultValue": "CSV"
			},
			"parquetdirectory": {
				"type": "string",
				"defaultValue": "Parquet"
			},
			"rangeofdatasetreportnameinfo": {
				"type": "string",
				"defaultValue": "B1:B3"
			},
			"rangeofdataset": {
				"type": "string"
			}
		},
		"variables": {
			"datasetname": {
				"type": "String"
			},
			"reportname": {
				"type": "String"
			}
		},
		"annotations": []
	}
}